{
  "preCI.localCodeCheck.prePush": "none",
  "preCI.localCodeCheck.filter.whitePath": "",
  "preCI.localCodeCheck.preCommit": "none",
  "preCI.localCodeCheck.filter.skipPath": ".*/\\.codecc/.*,.*/\\.git,.*/\\.git/.*,.*/\\.svn/.*",
  "preCI.pipeline.yamlFilePath": "build.yml",
  "preCI.localCodeCheck.checkerSet.JS": [
    "standard_js"
  ],
  "preCI.localCodeCheck.checkerSet.Golang": [
    "standard_go"
  ],
  "preCI.localCodeCheck.checkerSet.TS": [
    "ts_standard"
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "html",
    "vue"
  ],
  "eslint.alwaysShowStatus": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "editor.detectIndentation": false,
  "editor.tabSize": 4,
  "css.validate": false,
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "editor.rulers": [
    120
  ],
  "editor.selectionHighlight": true
}
